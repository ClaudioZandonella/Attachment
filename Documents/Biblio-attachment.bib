  keywords = {bayesian networks,expert opinions,linear pooling,posterior pooling,prior pooling,wayfinding},
  file = {/Users/claudio/MEGA/Zotero/Farr et al_2018_Prior and Posterior Linear Pooling for Combining Expert Opinions.pdf}
}

