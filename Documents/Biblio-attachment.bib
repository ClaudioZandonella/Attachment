  title = {Guidance on {{Expert Knowledge Elicitation}} in {{Food}} and {{Feed Safety Risk Assessment}}: {{Guidance}} on Expert Knowledge Elicitation},
  shorttitle = {Guidance on {{Expert Knowledge Elicitation}} in {{Food}} and {{Feed Safety Risk Assessment}}},
  author = {{European Food Safety Authority}},
  date = {2014-06},
