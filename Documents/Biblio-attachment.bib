@article{altinisikEvaluationInequalityConstrained2021,
  title = {Evaluation of Inequality Constrained Hypotheses Using a Generalization of the {{AIC}}.},
  author = {Altinisik, Yasin and Van Lissa, Caspar J. and Hoijtink, Herbert and Oldehinkel, Albertine J. and Kuiper, Rebecca M.},
