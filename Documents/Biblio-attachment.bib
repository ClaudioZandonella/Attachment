  file = {/Users/claudio/MEGA/Zotero/Bürkner_2018_Advanced Bayesian multilevel modeling with the R package brms.pdf;/Users/claudio/MEGA/Zotero/Bürkner_2018_Advanced Bayesian multilevel modeling with the R package brms2.pdf}
